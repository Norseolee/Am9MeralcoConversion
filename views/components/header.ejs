    <% if (message && message.length > 0) { %>
        <div class="alert alert-<%= message[0].type %> mt-3" role="alert">
            <%= message[0].text %>
        </div>
    <% } %>
<script>
    setTimeout(() => {
        const alert = document.querySelector('.alert');
        if (alert) {
            alert.style.display = 'none';
        }
    }, 3000);
</script>