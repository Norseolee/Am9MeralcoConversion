<!-- tenant_list.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tenant List</title>
</head>
<body>
    <%- include('../components/navbar.ejs') %>
    <h1>Tenant List</h1>
    <ul>
        <% tenants.forEach(tenant => { %>
            <li><%= tenant.name %>|| <%= tenant.building %></li>
        <% }); %>
    </ul>
</body>
</html>
